programa
{
	
	funcao inicio()
	{
		real variableN

		escreva("Digite o valor da variável: ")
		leia(variableN)

		se(variableN > 100){
			escreva(variableN)
		}
		senao{
			variableN = 0
			escreva(variableN)
		}
	}
}
